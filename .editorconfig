# editorconfig.org
root = true

[*.{js,vue}]
quote_type = double
indent_size = 4
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false
spaces_around_operators = true
curly_bracket_next_line = true
max_line_length = off
[*.md]
trim_trailing_whitespace = false
